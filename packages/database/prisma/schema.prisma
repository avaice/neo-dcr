// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Organization {
  id                          String    @id @default(uuid()) @map("id")
  name                        String    @map("name")
  shortName                   String    @map("short_name")
  prefectureCode              Int       @map("prefecture_code")
  medicalInstitutionCode      String    @map("medical_institution_code")
  facilityStandardNotificationCode String @map("facility_standard_notification_code")
  phoneNumber                 String?   @map("phone_number")
  createdAt                   DateTime  @default(now()) @map("created_at")
  updatedAt                   DateTime  @updatedAt @map("updated_at")

  patients                    Patient[]
  organizationUsers           OrganizationUser[]

  @@map("organization")
}

model OrganizationUser {
  id                          String    @id @default(uuid()) @map("id")
  userId                      String    @unique @map("user_id")
  passwordHash                String    @map("password_hash")
  organizationId              String    @map("organization_id")
  createdAt                   DateTime  @default(now()) @map("created_at")
  updatedAt                   DateTime  @updatedAt @map("updated_at")

  organization                Organization @relation(fields: [organizationId], references: [id])

  @@index([organizationId])
  @@map("organization_user")
}

model Patient {
  id                          String    @id @default(uuid()) @map("id")
  organizationId              String    @map("organization_id")
  firstName                   String    @map("first_name")
  lastName                    String    @map("last_name")
  kanaFirstName               String    @map("kana_first_name")
  kanaLastName                String    @map("kana_last_name")
  gender                      Int       @map("gender")
  dateOfBirth                 DateTime  @map("date_of_birth") @db.Date
  phoneNumber                 String?   @map("phone_number")
  createdAt                   DateTime  @default(now()) @map("created_at")
  updatedAt                   DateTime  @updatedAt @map("updated_at")
  deletedAt                   DateTime? @map("deleted_at")

  organization                Organization @relation(fields: [organizationId], references: [id])

  @@index([organizationId])
  @@map("patient")
}

